{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Flask",
      "type": "debugpy",
      "request": "launch",
      "module": "flask",
      "env": {
          "FLASK_APP": "ops_api.ops:create_app()",
          "FLASK_DEBUG": "1",
          "FLASK_RUN_PORT": "8080",
          "PYTHONPATH": "./backend"
      },
      "args": [
          "run"
      ],
      "jinja": true,
      "justMyCode": true
  }
  ]
}
